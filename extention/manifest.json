{
  "manifest_version": 2,
  "name": "Affiliate_amazone",
  "description": "Be able to press buttons",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["results.json", "util.js"],
  "permissions": [{ "fileSystem": ["write"] }, "tabs", "<all_urls>"]
}
